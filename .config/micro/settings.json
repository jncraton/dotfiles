        "https://raw.githubusercontent.com/micro-editor/plugin-channel/master/channel.json"
    ],
    "pluginrepos": [],
    "rmtrailingws": false,
    "ruler": false,
    "savecursor": false,
    "saveundo": false,
    "scrollmargin": 5,

{
    "*.go": {
        "indentchar": "\t",
        "tabstospaces": false
    },
    "*.tsv": {
        "indentchar": "\t",
        "tabsize": 4,
        "tabstospaces": false
    },
    "autoclose": false,
    "autoindent": true,
    "autosave": false,
    "colorcolumn": 73,
    "colorscheme": "zenburn",
    "cursorline": false,
    "eofnewline": false,
    "fileformat": "unix",
    "ftoptions": true,
    "gofmt": true,
    "goimports": false,
    "ignorecase": true,
    "indentchar": " ",

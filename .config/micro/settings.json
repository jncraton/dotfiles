{
    "*.go": {
        "indentchar": "\t",
        "tabstospaces": false
    },
    "*.tsv": {
        "indentchar": "\t",
        "tabsize": 4,
        "tabstospaces": false
    },
    "autoclose": false,
    "autoindent": true,
    "autosave": false,
    "basename": false,

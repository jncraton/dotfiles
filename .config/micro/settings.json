{
    "*.go": {
        "indentchar": "\t",
        "tabstospaces": false
    },
    "*.py": {
        "tabsize": 4,
        "tabstospaces": true
    },
    "*.tsv": {
        "indentchar": "\t",
        "tabsize": 4,
        "tabstospaces": false
    },
    "autoclose": false,
    "autoindent": true,
    "autosave": 0,
    "autosu": false,
    "backup": true,
    "basename": false,
    "colorcolumn": 0,
    "colorscheme": "simple",
    "comment": true,
    "cursorline": false,
    "diff": true,
    "diffgutter": false,
    "encoding": "utf-8",
    "eofnewline": false,
    "fastdirty": true,
    "fileformat": "unix",
    "filetype": "unknown",
    "ftoptions": true,
    "gofmt": true,
    "goimports": false,
    "ignorecase": true,
    "indentchar": " ",
    "infobar": true,
    "keepautoindent": false,
    "keymenu": false,
    "linter": false,
    "literate": true,
    "makefile": {
        "indentchar": "\t",
        "tabstospaces": false
    },
    "matchbrace": true,
    "mkparents": false,
    "mouse": true,
    "paste": false,
    "pluginchannels": [
        "https://raw.githubusercontent.com/micro-editor/plugin-channel/master/channel.json"
    ],
    "pluginrepos": [],
    "readonly": false,
    "rmtrailingws": false,
    "ruler": true,
    "savecursor": true,
    "savehistory": false,
    "saveundo": false,
    "scrollbar": true,
    "scrollmargin": 5,
    "scrollspeed": 5,
    "smartpaste": true,
    "softwrap": true,
    "splitbottom": true,
    "splitright": true,
    "status": true,
    "statusformatl": "$(filename) $(modified)($(line),$(col)) $(status.paste)| ft:$(opt:filetype) | $(opt:fileformat) | $(opt:encoding)",
    "statusformatr": "$(bind:ToggleKeyMenu): bindings, $(bind:ToggleHelp): help",
    "statusline": true,
    "sucmd": "sudo",
    "syntax": true,
    "tabmovement": false,
    "tabsize": 2,
    "tabstospaces": true,
    "termtitle": false,
    "useprimary": true,
    "xterm": false
}

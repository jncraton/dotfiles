{
  "*.tsv": {
    "tabsize": 4,
    "tabstospaces": false
  },
  "makefile": {
    "tabstispaces": false
  },
  "autoclose": false,
  "autoindent": true,
  "colorscheme": "zenburn",
  "cursorline": true,
  "gofmt": true,
  "goimports": false,
  "ignorecase": true,
  "indentchar": " ",
  "infobar": true,
  "linter": true,
  "ruler": true,
  "savecursor": true,
  "saveundo": false,
  "scrollmargin": 3,
  "scrollspeed": 2,
  "statusline": true,
  "syntax": true,
  "tabsize": 2,
  "tabstospaces": true
}